{
  "name": "8chanss",
  "version": "1.53.0",
  "description": "A userscript to add functionality to 8chan.",
  "meta": {
    "name": "8chanSS",
    "repo": "https://github.com/otacoo/8chanSS/",
    "page": "https://github.com/otacoo/8chanSS/",
    "downloads": "https://github.com/otacoo/8chanSS/releases/latest/download/",
    "buildsPath": "builds/",
    "mainBranch": "main",
    "matches": [
      "*://8chan.moe/*",
      "*://8chan.se/*"
    ],
    "exclude_matches": [
      "*://8chan.moe/login.html",
      "*://8chan.se/login.html"
    ],
    "files": {
      "metajs": "8chanSS.meta.js",
      "userjs": "8chanSS.user.js"
    },
    "min": {
      "chrome": "105",
      "firefox": "121",
      "greasemonkey": "4.3"
    }
  },
  "devDependencies": {
    "grunt": "^1.6.1",
    "grunt-bump": "^0.8.0",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-concat": "~2.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^5.0.0",
    "grunt-remove-comments": "^0.1.6",
    "grunt-shell": "~4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/otacoo/8chanSS.git"
  },
  "author": "otakudude",
  "contributors": [
    "bapao1"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=0.10"
  }
}
